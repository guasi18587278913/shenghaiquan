{
  "kind": "collectionType",
  "collectionName": "sub_lessons",
  "info": {
    "singularName": "sub-lesson",
    "pluralName": "sub-lessons",
    "displayName": "SubLesson"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "blocks"
    },
    "order": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "videoUrl": {
      "type": "string"
    },
    "lesson": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "sub_lessons"
    }
  }
}
